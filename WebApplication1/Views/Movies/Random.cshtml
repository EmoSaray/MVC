@model WebApplication1.ViewsModels.RandomMoiveViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Model.Movie.Name

@if (Model.Customers.Count() == 0)
{
    <text>Няма нищо за показване</text>
}
else
{
<ul>
    @foreach (var customer in Model.Customers)
    {
        <li>
            @customer.Name
        </li>
    }

</ul>

}

